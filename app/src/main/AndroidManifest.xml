<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.jeevan.hackforfood">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name="com.example.jeevan.hackforfood.activities.AppContext"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.jeevan.hackforfood.activities.OccasionsActivity" />
        <activity android:name="com.example.jeevan.hackforfood.activities.SearchActivity" />
        <activity android:name="com.example.jeevan.hackforfood.activities.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.jeevan.hackforfood.activities.SearchResultActivity" />
        <activity android:name="com.example.jeevan.hackforfood.activities.CheckoutCartActivity" />
        <activity android:name="com.example.jeevan.hackforfood.activities.OrderHomeActivity" />
        <activity android:name="com.example.jeevan.hackforfood.activities.OrderDetailsActivity"></activity>
    </application>

</manifest>